{
  "name": "otttooming.github.io",
  "private": false,
  "description": "Personal portfolio",
  "version": "0.1.0",
  "author": "Ott Tooming <otttooming@gmail.com>",
  "dependencies": {
    "@chakra-ui/gatsby-plugin": "^1.0.0",
    "@chakra-ui/react": "^1.0.4",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@mdx-js/mdx": "^1.6.6",
    "@mdx-js/react": "^1.6.6",
    "framer-motion": "^3.1.1",
    "gatsby": "^2.23.20",
    "gatsby-image": "^2.4.12",
    "gatsby-plugin-emotion": "^5.1.0",
    "gatsby-plugin-manifest": "^2.4.17",
    "gatsby-plugin-mdx": "^1.2.22",
    "gatsby-plugin-react-helmet": "^3.3.9",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^2.6.17",
    "gatsby-plugin-typescript": "^2.4.13",
    "gatsby-remark-copy-linked-files": "^2.3.10",
    "gatsby-remark-images": "^3.3.17",
    "gatsby-source-filesystem": "^2.3.18",
    "gatsby-transformer-sharp": "^2.5.10",
    "graphql": "14.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.8",
    "react-helmet": "^6.1.0",
    "react-medium-image-zoom": "^4.3.1",
    "recharts": "^1.8.5",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.16.0",
    "@graphql-codegen/typescript": "^1.16.0",
    "@graphql-codegen/typescript-operations": "^1.16.0",
    "@types/gh-pages": "^3.0.0",
    "@types/mdx-js__react": "^1.5.2",
    "@types/prettier": "^2.0.1",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/recharts": "^1.8.13",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gh-pages": "^3.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "keywords": [
    "gatsby",
    "react",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build:graphql": "graphql-codegen",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "deploy": "npm run build && gh-pages -d public -b master",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "prettier": "prettier --write --config .prettierrc \"**/*.{js,jsx,ts,tsx,json,css,scss,md,mdx,yaml,yml}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/otttooming/otttooming.github.io"
  },
  "bugs": {
    "url": "https://github.com/otttooming/otttooming.github.io/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{md,mdx,yaml,yml}": [
      "npm run prettier"
    ],
    "*.{js,jsx,ts,tsx,json,css,scss}": [
      "npm run prettier",
      "npm run lint"
    ]
  }
}
