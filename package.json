{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@coterminous/ui": "github:otttooming/coterminous-ui#219968c",
    "@mdx-js/mdx": "^1.3.0",
    "@mdx-js/react": "^1.3.0",
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "^2.13.51",
    "gatsby-image": "^2.2.8",
    "gatsby-plugin-manifest": "^2.2.4",
    "gatsby-plugin-mdx": "^1.0.24",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-react-helmet": "^3.1.3",
    "gatsby-plugin-react-svg": "^2.1.2",
    "gatsby-plugin-sharp": "^2.2.9",
    "gatsby-plugin-styled-components": "^3.1.2",
    "gatsby-plugin-typescript": "^2.1.2",
    "gatsby-remark-images": "^3.1.12",
    "gatsby-source-filesystem": "^2.1.8",
    "gatsby-transformer-sharp": "^2.2.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-feather": "^2.0.3",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.3.2",
    "styled-system": "^5.0.21",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@types/prettier": "^1.18.1",
    "@types/react": "^16.8.25",
    "@types/react-dom": "^16.8.5",
    "@types/react-helmet": "^5.0.9",
    "@types/styled-components": "^4.1.18",
    "@types/styled-system": "^5.0.1",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.3",
    "prettier": "^1.18.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "deploy": "npm run build && gh-pages -d public -b master",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "prettier": "prettier --write --config .prettierrc \"**/*.{js,jsx,ts,tsx,json,css,scss,md,yaml,yml}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md,yaml,yml}": [
      "npm run prettier",
      "git add"
    ]
  }
}
